{
  "name": "@wgu-extension/graphql-client",
  "version": "0.2.0",
  "private": false,
  "type": "module",
  "description": "GraphQL client and types for WGU Extension API",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonshaffer/wgu-extension.git"
  },
  "bugs": {
    "url": "https://github.com/jonshaffer/wgu-extension/issues"
  },
  "homepage": "https://github.com/jonshaffer/wgu-extension#readme",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "pnpm run clean && tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.js --fix",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@wgu-extension/functions": "workspace:*",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@wgu-extension/data": "workspace:*"
  }
}