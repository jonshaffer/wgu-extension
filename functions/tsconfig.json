{
  "compilerOptions": {
  "module": "NodeNext",
    "esModuleInterop": true,
    "moduleResolution": "nodenext",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
  "strict": true,
  "skipLibCheck": true,
  "target": "ES2022",
  "declaration": true,
  "declarationMap": true
  },
  "compileOnSave": true,
  "include": [
    "src"
  ]
}
