{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "declaration": true,
    "emitDeclarationOnly": false,
    "composite": false,
    "noEmit": false,
  "allowImportingTsExtensions": false,
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "Bundler",
    "isolatedModules": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*", "src/canonical-raw.d.ts"],
  "files": ["src/canonical-raw.d.ts"],
  "exclude": ["src/canonical-types.ts"]
}
