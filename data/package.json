{
  "name": "@wgu-extension/data",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "description": "Data collection, processing, and shared TypeScript types for WGU Extension",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonshaffer/wgu-extension.git"
  },
  "bugs": {
    "url": "https://github.com/jonshaffer/wgu-extension/issues"
  },
  "homepage": "https://github.com/jonshaffer/wgu-extension#readme",
  "sideEffects": false,
  "main": "types/dist/index.js",
  "module": "types/dist/index.js",
  "types": "types/dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./types/dist/index.js",
      "require": "./types/dist/index.js",
      "types": "./types/dist/index.d.ts"
    },
    "./canonical-raw": {
      "types": "./types/dist/canonical-raw.d.ts"
    }
  },
  "files": [
    "types/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev-setup": "tsx scripts/dev-setup.ts",
    "download:samples": "tsx scripts/download-samples.ts",
    "populate:dev-cache": "tsx scripts/populate-dev-cache.ts",
    "test": "echo 'Running tests...' && npm run test:catalog && npm run test:collection",
    "test:catalog": "tsx pipelines/catalog/test.ts",
    "test:collection": "tsx collection/test.ts",
    "test:parser": "tsx pipelines/catalog/test-parser.ts",
    "test:ci": "npm test -- --ci",
    "parse:catalog": "tsx pipelines/catalog/parse.ts",
    "health:catalog": "tsx pipelines/catalog/analytics/generate-report.ts",
    "validate:all": "npm run validate:discord && npm run validate:reddit && npm run validate:catalog",
    "validate:catalog": "tsx pipelines/catalog/validate.ts",
    "validate:ci": "npm run validate:all -- --ci",
    "preview": "tsx pipelines/unified/preview.ts",
    "monitor:health": "tsx monitors/run-all.ts",
    "ingest": "npm run dvc:pull && npm run ingest:discord && npm run ingest:reddit && npm run ingest:wgu-connect && npm run ingest:catalog",
    "ingest:discord": "tsx discord/scripts/ingest-discord.ts",
    "ingest:reddit": "tsx reddit/scripts/ingest-reddit.ts",
    "ingest:wgu-connect": "tsx wgu-connect/scripts/ingest-wgu-connect.ts",
    "ingest:wgu-student-groups": "tsx wgu-student-groups/scripts/ingest-wgu-student-groups.ts",
    "ingest:catalog": "tsx catalogs/scripts/ingest-catalogs.ts",
    "ingest:catalog:v1": "tsx catalogs/scripts/ingest-catalogs.ts --kind legacy",
    "ingest:catalog:auto": "tsx catalogs/scripts/ingest-catalogs.ts --kind auto",
    "validate:discord": "tsx discord/scripts/validate-raw.ts --discord",
    "validate:discord:check-invites": "tsx discord/scripts/validate-raw.ts --discord --check-invites",
    "validate:reddit": "tsx reddit/scripts/validate-raw.ts --reddit",
    "update:discord": "tsx discord/scripts/validate-raw.ts --discord --update",
    "update:reddit": "tsx reddit/scripts/validate-raw.ts --reddit --update",
    "transform": "tsx unified/scripts/transform-unified.ts",
    "dvc:pull": "./scripts/ensure-dvc-data.sh",
    "catalog:ensure": "./scripts/ensure-dvc-data.sh",
    "catalog:report": "./scripts/ensure-dvc-data.sh && tsx catalogs/scripts/catalog-reporting.ts",
    "catalog:parse": "tsx catalogs/scripts/catalog-parser-unified.ts",
    "catalog:batch": "tsx catalogs/scripts/batch-parse-all.ts",
    "catalog:generate-processed": "tsx catalogs/scripts/generate-processed-data.ts",
    "upload:academic-registry": "tsx scripts/upload-academic-registry.ts",
    "upload:wgu-student-groups": "tsx scripts/upload-wgu-student-groups.ts",
    "upload:all": "npm run upload:academic-registry && npm run upload:wgu-student-groups",
    "verify:firestore": "tsx scripts/verify-firestore-data.ts",
    "typecheck": "tsc --noEmit",
    "types:clean": "rm -rf types/dist",
    "types:build:esm": "tsc -p types/tsconfig.json",
    "types:build": "npm run types:clean && npm run types:build:esm && cp types/src/canonical-raw.d.ts types/dist/canonical-raw.d.ts",
    "build": "npm run types:build",
    "prepublishOnly": "npm run build",
    "version:check": "npm view @wgu-extension/data versions --json"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "firebase-admin": "^13.4.0",
    "pdf-parse": "^1.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.1",
    "jsdom": "^26.1.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
