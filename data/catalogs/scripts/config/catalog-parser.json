{
  "environment": "development",
  "parsing": {
    "maxRetries": 3,
    "retryDelayMs": 1000,
    "timeoutMs": 120000,
    "batchSize": 5,
    "minCoursesPerCatalog": 100,
    "minCCNsCoveragePercent": 80,
    "minCUsCoveragePercent": 70,
    "memoryLimitMB": 2048,
    "maxConcurrentParsing": 2,
    "preserveDetailedDescriptions": true,
    "outputIndentation": 2,
    "compressOutput": false
  },
  "download": {
    "userAgent": "WGU-Catalog-Parser/2.0 (Educational Research)",
    "maxRetries": 3,
    "retryDelayMs": 3000,
    "timeoutMs": 180000,
    "requestsPerSecond": 2,
    "burstLimit": 5,
    "maxFileSizeMB": 50,
    "tempDirectory": "temp",
    "cleanupTempFiles": true,
    "validatePDFSignature": true,
    "minPDFSizeKB": 100
  },
  "logging": {
    "consoleLevel": "DEBUG",
    "fileLevel": "DEBUG",
    "logDirectory": "logs",
    "maxLogFileSizeMB": 100,
    "maxLogFiles": 10,
    "includeTimestamp": true,
    "includeStackTrace": true,
    "colorOutput": true
  },
  "monitoring": {
    "enableHealthChecks": false,
    "healthCheckIntervalMs": 60000,
    "enableMetrics": false,
    "metricsPort": 9090,
    "enableAlerting": false,
    "alertThresholds": {
      "parseFailureRate": 0.1,
      "downloadFailureRate": 0.05,
      "memoryUsagePercent": 0.85
    }
  },
  "paths": {
    "catalogsDirectory": "pdfs",
    "parsedDirectory": "parsed",
    "outputDirectory": "output",
    "tempDirectory": "temp",
    "configDirectory": "config"
  },
  "features": {
    "enableLegacyParsing": true,
    "enableDetailedDescriptions": true,
    "enableCCNExtraction": true,
    "enableCUExtraction": true,
    "enableDegreePrograms": true,
    "enableValidation": false,
    "enableMetrics": false
  }
}